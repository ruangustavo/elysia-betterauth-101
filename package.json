{
	"name": "app",
	"version": "1.0.50",
	"scripts": {
		"dev": "bun --env-file=.env --watch run src/index.ts",
		"typecheck": "tsc --noEmit",
		"db:generate": "bunx --bun drizzle-kit generate",
		"db:migrate": "npx drizzle-kit migrate",
		"auth:generate": "bunx --bun @better-auth/cli generate --config ./src/auth --yes",
		"biome:fix": "biome check --write ./src"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/openapi": "^1.4.3",
		"better-auth": "^1.3.12",
		"bun": "^1.2.22",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.5",
		"elysia": "latest",
		"zod": "^4.1.9"
	},
	"devDependencies": {
		"@better-auth/cli": "^1.3.12",
		"@biomejs/biome": "2.2.4",
		"bun-types": "latest"
	},
	"module": "src/index.js"
}
